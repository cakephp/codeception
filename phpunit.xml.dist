<?xml version="1.0" encoding="UTF-8"?>
<phpunit
  colors="true"
  processIsolation="false"
  stopOnFailure="false"
  syntaxCheck="false"
  bootstrap="tests/bootstrap.php"
  strict="true"
>
  <php>
    <ini name="memory_limit" value="-1"/>
  </php>

  <testsuites>
    <testsuite name="App Test Suite">
      <directory>./tests/TestCase</directory>
    </testsuite>
  </testsuites>

</phpunit>
